{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [],
  "dataGroups": [
    {
      "name": "data",
      "urls": [
        "https://api.github.com/repos/*/git/blobs/*",
        "http://localhost:5179/repos/*/blobs/*"
      ],
      "cacheConfig": {
        "maxSize": 1000,
        "maxAge": "365d",
        "strategy": "performance"
      }
    },
    {
      "name": "protomaps-api",
      "urls": [
        "https://api.protomaps.com/**"
      ],
      "cacheConfig": {
        "maxSize": 1000,
        "maxAge": "365d",
        "strategy": "freshness"
      }
    }
  ]
}
