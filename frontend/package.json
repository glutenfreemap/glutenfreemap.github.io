{
  "name": "glutenfreemap",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n{/pt,/en,/fr,/es}.json --clean --format json",
    "generate-style": "tsx generate_style.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.1.6",
    "@angular/common": "^20.1.7",
    "@angular/compiler": "^20.1.7",
    "@angular/core": "^20.1.7",
    "@angular/forms": "^20.1.7",
    "@angular/material": "^20.1.6",
    "@angular/platform-browser": "^20.1.7",
    "@angular/platform-browser-dynamic": "^20.1.7",
    "@angular/router": "^20.1.7",
    "@angular/service-worker": "^20.1.7",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@octokit/rest": "^21.1.1",
    "maplibre-gl": "^5.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "type-fest": "^4.41.0",
    "zone.js": "~0.15.0"
  },
