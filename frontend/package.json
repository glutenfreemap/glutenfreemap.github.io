    "@types/jasmine": "~5.1.0",
    "@vendure/ngx-translate-extract": "^10.0.1",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tsx": "^4.19.3",
    "typescript": "~5.8.3",
    "zod": "^4.1.1"
  }
